def aggregate_outer(data):

    n_nodes = data.shape[0]

    out = jnp.zeros((n_nodes, n_nodes), dtype=np.float32)

    def _outer(carry, x):
        carry = carry + jnp.outer(x, x)
        return carry, None

    carry, _ = jax.lax.scan(_outer, out, data.T)

    carry = np.array(carry)
    np.fill_diagonal(carry, 0)

    return carry


